{
    "manifest_version": 3,
    "name": "Random Notifier",
    "version": "1.0",
    "description": "Sends notifications at random intervals",
    "permissions": [
        "notifications",
        "alarms",
        "tts",
        "ttsEngine"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "128": "icon.png"
    }
}